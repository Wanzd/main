<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pd.it.system.user.userBase.IListUserDao">

	<select id="ra" resultType="VO">
		<include refid="sql$r"/>
	</select>
	
	<sql id="sql$r">
SELECT name as "id",
	name as "text"
FROM user_base_t
order by name
	</sql>
</mapper>  